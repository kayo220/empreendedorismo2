<!DOCTYPE html>
<html lang="en">
<head>
<% include ../common/default-page-head %>
</head>
<body>

	<!-- header -->
	<% include ../common/header.ejs %>
	<!-- //header -->
	<!-- banner -->

	<div class="products-row">
		<%for(var id in houses){
			var house = houses[id];
			%>
			<div class="col-md-3 product-grids">
				<div class="agile-products">
					<a href="/house/view/<%= id %>"><img src="/<%= house.photos[0]%>" class="img-responsive" alt="img"></a>
					<div class="agile-product-text">
						<h5><a href="/house/view/<%= id %>"><%= house.housename%></a></h5>
						<h6>R$ <%= house.rent%></h6>
					</div>
				</div>
			</div>
		<% }%>
		<div class="clearfix"> </div>
	</div>
	<!-- //banner -->
	<!-- welcome -->

	<!-- footer -->
	<% include ./../common/footer %>
	<% include ./../common/libs %>

</body>
</html>
